<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="org.debz.model.User" table="users">
        <id name="sid" type="long" column="sid" >
            <generator class="increment"/>
        </id>
        <property name="f_name">
            <column name="f_name" />
        </property>
        <property name="o_name">
            <column name="o_name"/>
        </property>
        <property name="username">
            <column name="username"/>
        </property>
        <property name="password">
            <column name="password"/>
        </property>
        <property name="date">
            <column name="date"/>
        </property>
        <property name="level">
            <column name="level"/>
        </property>
        <property name="station">
            <column name="station"/>
        </property>
        <property name="email">
            <column name="email"/>
        </property>
    </class>


    <class name="org.debz.model.Course" table="courses">
        <id name="sid" type="long" column="sid" >
            <generator class="increment"/>
        </id>
        <property name="name">
            <column name="name" />
        </property>
        <property name="units">
            <column name="units"/>
        </property>
        <property name="units_percentage">
            <column name="units_percentage"/>
        </property>

    </class>
</hibernate-mapping>